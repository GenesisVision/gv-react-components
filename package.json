{
  "name": "gv-react-components",
  "version": "0.0.28",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "styles": "dist/index.css",
  "author": "nkrivous <nikolai@genesis.vision>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GenesisVision/gv-react-components.git"
  },
  "keywords": [
    "TypeScript",
    "Storybook",
    "StoryShots",
    "React"
  ],
  "scripts": {
    "format": "prettier --write 'src/**/*'",
    "test": "jest",
    "type-sass": "atsm -p \"src/**/*.scss\" -c",
    "copy-sass": "cpx \"src/**/*.scss\" dist",
    "copy-assets": "cpx \"assets/**/*.*\" \"dist/assets\"",
    "prebuild": "rimraf dist && yarn type-sass && yarn copy-sass",
    "build": "rollup -c",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs"
  },
  "devDependencies": {
    "@dump247/storybook-state": "^1.5.0",
    "@storybook/addon-actions": "^3.4.3",
    "@storybook/addon-info": "^3.4.7",
    "@storybook/addon-links": "^3.4.3",
    "@storybook/addon-storyshots": "^3.4.3",
    "@storybook/addons": "^3.4.3",
    "@storybook/react": "^3.4.3",
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.12",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.2.3",
    "@types/prop-types": "^15.5.3",
    "@types/query-string": "^5.1.0",
    "@types/react-router-dom": "^4.2.6",
    "@types/storybook__react": "^3.0.7",
    "awesome-typed-sass-modules": "^1.0.4",
    "awesome-typescript-loader": "^5.0.0",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "camelcase": "^5.0.0",
    "cpx": "^1.5.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "formik": "^1.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "jest-transform-stub": "^1.0.0",
    "moment": "^2.22.2",
    "node-sass": "^4.9.3",
    "prettier": "^1.12.1",
    "query-string": "^6.0.0",
    "react-dom": "^16.4.0",
    "react-number-format": "^3.6.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-typescript2": "^0.15.0",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-plugin-url": "^1.4.0",
    "ts-jest": "^22.4.4",
    "tslib": "^1.9.0",
    "typescript": "^2.8.3",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.6.1",
    "react": "^16.4.1"
  },
  "peerDependencies": {
    "formik": "^1.0.2"
  }
}
